{{> footer}}

		<form
			netlify
			id="cta"
			name="contact"
			class="modal"
			action="/kontaktformular-danke.html"
			method="POST"
		>
			<a href="#" class="modalClose" hidden aria-hidden="true"></a>
			<div>
				<section>
					<a href="#" class="modalClose" hidden aria-hidden="true"></a>
					<h2>Kostenloses Beratungsgespräch vereinbaren</h2>
					<p>
						Erhalten Sie eine kostenlose Beratung von unseren Experten und
						lassen Sie uns gemeinsam herausfinden, wie wir Ihnen helfen können.
					</p>
					<div>
						<fieldset>
							<label class="honey-field">
								Email
								<input type="email" name="honeypot" />
							</label>
							<label>
								Name (optional)<br />
								<input name="name" /><br />
							</label><label>
								Email<br />
								<input type="email" name="userName" required /><br />
							</label>
							<label>
								Telefon (optional)<br />
								<input type="tel" name="phone" /><br />
							</label>

							<label>
								Ihre Nachricht (optional)<br />
								<textarea name="message" rows="5" cols="33"></textarea>
							</label>
						</fieldset>

						<button>Abschicken</button>
					</div>
				</section>
			</div>
		</form>
    
    {{> consentModal}}
    <a href="#top" id="scroll-to-top">🔝</a>
